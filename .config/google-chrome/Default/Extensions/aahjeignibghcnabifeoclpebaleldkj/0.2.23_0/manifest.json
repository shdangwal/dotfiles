{
   "action": {
      "default_title": "Open Vertical tabs (Alt + V)"
   },
   "author": "TabRail",
   "background": {
      "service_worker": "background/background.js",
      "type": "module"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Alt+V",
            "linux": "Alt+V",
            "mac": "Alt+V",
            "windows": "Alt+V"
         }
      },
      "duplicateTab": {
         "description": "Duplicate current tab",
         "suggested_key": {
            "default": "Alt+C"
         }
      }
   },
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "homepage_url": "https://tabVertikal.tabrail.site/",
   "icons": {
      "128": "icon_128.png",
      "16": "icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "123",
   "name": "__MSG_extName__",
   "offline_enabled": true,
   "options_page": "options/index.html",
   "permissions": [ "sidePanel", "tabs", "tabGroups", "storage", "contextMenus" ],
   "short_name": "tabVertikal",
   "side_panel": {
      "default_path": "panel/index.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2.23"
}
