{
   "action": {
      "default_icon": "images/icons/icon-32.png"
   },
   "background": {
      "service_worker": "/js/background.js"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "/js/contentScript.js" ],
      "matches": [ "http://*/*", "https://*/*", "ftp://*/*", "file:///*", "*://*/*" ],
      "run_at": "document_start"
   } ],
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "images/icons/icon-128.png",
      "16": "images/icons/icon-16.png",
      "38": "images/icons/icon-32.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "JSON Viewer Pro",
   "offline_enabled": true,
   "options_page": "options.html",
   "permissions": [ "contextMenus", "storage" ],
   "short_name": "JSON Viewer",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.6",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*", "ftp://*/*", "file:///*" ],
      "resources": [ "/js/main.js", "/css/style.css", "/css/color-themes/dark-pro.css", "/css/color-themes/mdn-light.css", "/css/codemirror.css", "/images/icons/bell.svg", "options.html" ]
   } ]
}
